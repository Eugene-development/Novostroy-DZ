{
  "name": "app",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "rm -rf build && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview --host --port 9999",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "start": "PORT=9999 node build/index.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.46.0",
    "svelte-motion": "^0.11.2",
    "svelte-preprocess": "^4.10.3",
    "tailwindcss": "^3.0.23"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "@sveltejs/adapter-node": "^1.0.0-next.62",
    "cookie": "^0.4.1"
  }
